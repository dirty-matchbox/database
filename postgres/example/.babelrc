{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@dirty-matchbox/database": "../../*",
          "underscore": "lodash"
        }
      }
    ],
    "syntax-dynamic-import"
  ],
  "presets": [["env", { "modules": false }]],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
